{
  "inbounds": [
    {
      "listen": "0.0.0.0", 
      "port": 80, 
      "protocol": "vmess", 
      "settings": {
        "clients": [
          {
            "alterId": 1, 
            "id": "UUID", 
            "level": 1
          }
        ]
      }, 
      "streamSettings": {
        "network": "ws", 
        "wsSettings": {
          "path": "/UUID"
        }
      }
    }
  ], 
  "log": {
    "access": "/var/log/v2ray/access.log", 
    "error": "/var/log/v2ray/error.log", 
    "loglevel": "warning"
  }, 
  "outbounds": [
    {
      "protocol": "freedom", 
      "settings": { }
    }
  ]
}